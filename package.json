{
    "name": "@elizaos/plugin-sei-yield-delta",
    "version": "1.0.0",
    "description": "SEI Network yield optimization and DeFi trading plugin for Eliza",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "@elizaos/source": "./src/index.ts",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@elizaos/core": "^1.2.12",
        "dotenv": "^17.2.0",
        "ethers": "^6.15.0",
        "node-cache": "5.1.2",
        "tsup": "8.3.5",
        "typescript": "^5.8.3",
        "viem": "2.22.23",
        "vitest": "^3.2.4",
        "zod": "^3.25.76"
    },
    "scripts": {
        "build": "tsup --format esm --dts",
        "lint": "eslint . --fix",
        "test": "NODE_ENV=test vitest run",
        "test:watch": "NODE_ENV=test vitest --watch",
        "test:coverage": "vitest --coverage"
    },
    "keywords": [
        "eliza",
        "sei",
        "defi",
        "yield",
        "trading",
        "dragonswap",
        "arbitrage",
        "yield-farming",
        "sei-blockchain",
        "typescript"
    ],
    "peerDependencies": {
        "form-data": "4.0.1",
        "whatwg-url": "7.1.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "agentConfig": {
        "pluginType": "elizaos:client:1.0.0",
        "pluginParameters": {
            "SEI_PRIVATE_KEY": {
                "type": "string",
                "minLength": 1,
                "description": "Sui private key is required"
            },
            "SEI_NETWORK": {
                "type": "string",
                "minLength": 1,
                "description": "SEI_NETWORK must be one of the following: mainnet, testnet, devnet, local"
            }
        }
    },
    "devDependencies": {
        "@lifi/types": "^17.22.0",
        "@types/node": "^24.0.15",
        "@vitest/coverage-v8": "^3.2.4"
    }
}
